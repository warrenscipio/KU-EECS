public class Addition{

	public static void main (String args[]){
	
	system.out.println("will you type two whole numbers good sir?");
	
	int n1, n2;

	Scanner keyboard = new Scanner(System.in);
	n1 = keyboard.nextInt( );
	n2 = keyboard.nextInt( );

	System.out.println("THEY COMBINE AND MAKE....");
	system.out.println(n1 + n2);
	}
}

