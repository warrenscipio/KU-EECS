//echo displays to terminal
TCLASS = "all files"
CXXFLAGS = -c -g -std=c++11
CXX=g++
PROFILE = -fprofile-arcs -ftest-coveage 


tests: $(TCLASS) unitTest.cpp
	$(CXX) $(CXXFLAGS) $(PROFILE) unitTest.cpp


coverage:

	$(LCOV)-lcov


