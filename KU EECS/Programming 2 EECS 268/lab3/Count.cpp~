*/

#include "Count.h"
#include <string>

using namespace std;

template <typename T>
Count<T>::Count(){
	count = 0;

}



template <typename T>
void Count<T>::addTo(){
	count++;
}
template <typename T>
void Count<T>::placeItem(T t){
	item=t;
	count=1;
	

}

template <typename T>
T Count<T>::getItem() const{
return item;
}


template <typename T>
int Count<T>::getCount() const{
	return count;
}

	

